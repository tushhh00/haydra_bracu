<link name="ee_link"/>
  <joint name="ee_fixed_joint" type="fixed">
    <parent link="gripper_1"/>
    <child link="ee_link"/>
    <origin xyz="0 0 0.225" rpy="0 0 0"/>
  </joint>




<joint name="joint_1" type="revolute">
  <origin xyz="0.0 0.0 0.3498" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="link_1_1"/>
  <axis xyz="0.0 0.0 1.0"/>
  <limit upper="1.745329" lower="-1.745329" effort="100" velocity="100"/>
</joint>

<joint name="joint_2" type="revolute">
  <origin xyz="0.0065 0.0 0.0962" rpy="0 0 0"/>
  <parent link="link_1_1"/>
  <child link="link_2_1"/>
  <axis xyz="-1.0 0.0 0.0"/>
  <limit upper="1.745329" lower="-0.872665" effort="100" velocity="100"/>
</joint>

<joint name="joint_3" type="revolute">
  <origin xyz="-0.0123 0.0 0.187" rpy="0 0 0"/>
