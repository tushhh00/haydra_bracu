<link name="gripper_1">
  <inertial>
    <origin xyz="6.152017482170002e-05 0.0008155438037858232 0.08968942355527643" rpy="0 0 0"/>
    <mass value="0.7956624906794072"/>
    <inertia ixx="0.002947" iyy="0.00294" izz="0.001036" ixy="-8e-06" iyz="-6e-06" ixz="2e-06"/>
