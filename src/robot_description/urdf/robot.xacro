<?xml version="1.0" ?>
<robot name="robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find robot_description)/urdf/materials.xacro" />
<xacro:include filename="$(find robot_description)/urdf/robot.ros2control.xml" />
<xacro:include filename="$(find robot_description)/urdf/robot.gazebo" />




<link name="world"/>
  <joint name="world_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>



<link name="base_link">
  <inertial>
    <origin xyz="0.0027874319748177083 -6.6482327611205835e-06 0.1693760086095525" rpy="0 0 0"/>
    <mass value="2.986783245883562"/>
    <inertia ixx="0.03718" iyy="0.037718" izz="0.023436" ixy="2e-06" iyz="3e-06" ixz="-0.001019"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find robot_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find robot_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

